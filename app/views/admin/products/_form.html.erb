<%= simple_form_for [:admin, @product] do |f| %>

<div class="group">
  <%= f.input :title %>
</div>

<div class="group">
  <%= f.input :description %>
</div>

<div class="group">
  <%= f.input :quantity %>
</div>

<div class="group">
  <%= f.input :price %>
</div>

<div class="picture_upload">
  <%= f.simple_fields_for :photo do |c| %>
  <%= c.input :image, as: :file %>
  <% end %>
</div>

<%= f.submit "送出", data: {disable_with: "Submitting"} %>

<% end %>
